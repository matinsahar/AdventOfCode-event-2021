    "a_file = open(\"Q1.txt\")\n",
    "file_contents = a_file.read()\n",
    "data = file_contents.splitlines()\n",
    "summ = 0\n",
    "d = list(map(int, data))\n",
    "print(sum(d[0:3]))\n",
    "for i in range(len(d)-3):\n",
    "    c= sum(d[i:i+3])\n",
    "    if c < sum(d[i+1:i+4]):\n",
    "        summ+=1\n",
    "print(summ, len(data))\n"
   ]
